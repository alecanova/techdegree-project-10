il);\nconst UserSignInWithContext = withContext(UserSignIn);\n\n\nexport default () => (\n    \n  <BrowserRouter>\n    <main>\n      <Switch>\n        <Route exact path= \"/\" component={CoursesWithContext} />\n        <Route path= \"/courses/:id\" component={CourseDetailWithContext} />\n        <Route path= \"/signin\" component={UserSignInWithContext} />\n      </Switch>\n    </main>\n  </BrowserRouter>\n\n)\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n","/mnt/c/Users/39334/documents/Full Stack JavaScript/techdegree-project-10/client/src/Data.js",[],"/mnt/c/Users/39334/documents/Full Stack JavaScript/techdegree-project-10/client/src/components/Courses.js",[],"/mnt/c/Users/39334/documents/Full Stack JavaScript/techdegree-project-10/client/src/config.js",["116"],"/mnt/c/Users/39334/documents/Full Stack JavaScript/techdegree-project-10/client/src/components/CourseDisplay.js",[],"/mnt/c/Users/39334/Documents/Full Stack JavaScript/techdegree-project-10/client/src/components/CourseDisplay.js",[],"/mnt/c/Users/39334/Documents/Full Stack JavaScript/techdegree-project-10/client/src/components/CourseDetail.js",["117"],"/mnt/c/Users/39334/documents/Full Stack JavaScript/techdegree-project-10/client/src/components/CourseDetail.js",["118"],"import React, {Component} from 'react';\r\n\r\n\r\n\r\nexport default class CourseDetail extends Component {\r\n\r\n    state = {\r\n        course: {},\r\n    }\r\n\r\n    componentDidMount() {\r\n        this.fetchCourse();\r\n    }\r\n\r\n    async fetchCourse() {\r\n\r\n        // gives access to the data in Data.js and actions in Context.js\r\n        const { context } = this.props;\r\n        const courseId = this.props.match.params.id;\r\n\r\n        try {\r\n            const course = await context.data.getCourse(courseId);\r\n            this.setState({ course })\r\n        } catch(error) {\r\n            console.log(error);\r\n        }\r\n    }\r\n\r\n    // DELETE request to the REST API's /api/courses/:id\r\n    deleteCourse = (id) => {\r\n\r\n        // gives access to the data in Data.js and actions in Context.js\r\n        const { context } = this.props;\r\n        const inputConfirmed = window.confirm('Are you sure you want to delete this course?');\r\n        \r\n        if(inputConfirmed) {\r\n            context.data.deleteCourse(id)\r\n                .then(data => {\r\n                    if(data) { \r\n                        // if deleted return at the home page\r\n                        this.props.history.push('/');\r\n                    }\r\n                })\r\n                .catch(error => {\r\n                    console.log(error);\r\n                });\r\n        }\r\n\r\n    };\r\n\r\n    render () {\r\n\r\n        const { course } = this.state;\r\n        const { user } = course;\r\n\r\n        let editButtons;\r\n\r\n        editButtons = (\r\n            <div className=\"actions--bar\">\r\n                <div className=\"bounds\">\r\n                    <div className=\"grid-100\">\r\n                        <button className=\"button\" onClick={() => this.props.history.push(`/courses/${course.id}/update`)}>\r\n                            Update Course\r\n                        </button>\r\n                        <button className=\"button\" onClick={() => this.deleteCourse(course.id)}>\r\n                            Delete Course\r\n                        </button>\r\n                        <button className=\"button button-secondary\" onClick={() => this.props.history.push('/')}>\r\n                            Return to List\r\n                        </button>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        );\r\n\r\n        return (\r\n\r\n            <>\r\n                {editButtons}\r\n\r\n                <div className=\"bounds course--detail\">\r\n                    <div class=\"grid-66\">\r\n                        <div class=\"course--header\">\r\n                            <h4 className=\"course--label\">Course</h4>\r\n                            <h3 className=\"course--title\">{course.title}</h3>\r\n                            <p>By </p>\r\n                        </div>\r\n                        <div className=\"course--description\">{course.description}</div>\r\n                    </div>\r\n                    <div class=\"grid-25 grid-right\">\r\n                        <div class=\"course--stats\">\r\n                            <ul class=\"course--stats--list\">\r\n                                <li class=\"course--stats--list--item\">\r\n                                    <h4>Estimated Time</h4>\r\n                                    <h3>{course.estimatedTime}</h3>\r\n                                </li>\r\n                                <li class=\"course--stats--list--item\">\r\n                                    <h4>Materials Needed</h4>\r\n                                    <ul>\r\n                                        <li>{course.materialsNeeded}</li>\r\n                                    </ul>\r\n                                </li>\r\n                            </ul>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </>\r\n\r\n        );\r\n\r\n\r\n\r\n    }\r\n\r\n    \r\n\r\n}\r\n","/mnt/c/Users/39334/documents/Full Stack JavaScript/techdegree-project-10/client/src/components/UserSignIn.js",["119","120"],"import React, {Component} from 'react';\r\nimport { Link } from 'react-router-dom';\r\nimport Form from './Form';\r\n\r\n\r\n\r\n\r\nexport default class UserSignIn extends Component {\r\n\r\n    state = {\r\n        username: '',\r\n        password: '',\r\n        errors: [],\r\n    }\r\n\r\n\r\n    render () {\r\n\r\n        const { username, password, errors } = this.state;\r\n\r\n        return (\r\n            <div className=\"bounds\">\r\n                <div className=\"grid-33 centered signin\">\r\n                    <h1>Sign In</h1>\r\n                    <Form \r\n                        cancel={this.cancel}\r\n                        errors={errors} //da fare\r\n                        submit={this.submit} //da fare\r\n                        submitButtonText=\"Sign In\"\r\n                        elements={() => (\r\n                            <React.Fragment>\r\n                                <input \r\n                                    id=\"emailAddress\"\r\n                                    name=\"emailAddress\"\r\n                                    type=\"text\"\r\n                                    onChange={this.change}\r\n                                    placeholder=\"Email Address\" />\r\n                                <input \r\n                                    id=\"password\"\r\n                                    name=\"password\"\r\n                                    type=\"text\"\r\n                                    onChange={this.change}\r\n                                    placeholder=\"Password\" />    \r\n                            </React.Fragment>\r\n                        )}\r\n                    />\r\n                    <p>\r\n                        Don't have a user account? <Link to=\"/signup\">Click here</Link> to sign up!\r\n                    </p>\r\n                </div>\r\n            </div>\r\n        );\r\n\r\n    }\r\n\r\n    change = (event) => {\r\n        const name = event.target.name;\r\n        const value = event.target.value;\r\n\r\n        this.setState(() => {\r\n            return {\r\n                [name]: value\r\n            };\r\n        });\r\n    }\r\n\r\n    cancel = () => {\r\n        this.props.history.push(\"/\");\r\n    }\r\n\r\n\r\n\r\n\r\n\r\n\r\n}","/mnt/c/Users/39334/documents/Full Stack JavaScript/techdegree-project-10/client/src/components/Form.js",["121","122"],"import React from 'react';\r\n\r\n\r\nexport default (props) => {\r\n\r\n    const {\r\n        cancel,\r\n        errors, // errorDisplay da aggiungere\r\n        submit,\r\n        submitButtonText,\r\n        elements,\r\n    } = props;\r\n\r\n    function handleSubmit(event) {\r\n        event.preventDefault();\r\n        submit();\r\n    };\r\n\r\n    function handleCancel(event) {\r\n        event.preventDefault();\r\n        cancel();\r\n    };\r\n\r\n    return (\r\n        <div>\r\n            <form onSubmit={handleSubmit} >\r\n                {elements()}\r\n                <div>\r\n                    <button className=\"button\" type=\"submit\">{submitButtonText}</button>\r\n                    <button className=\"button button-secondary\" onClick={handleCancel}>Cancel</button>\r\n                </div>\r\n            </form>\r\n        </div>\r\n    );\r\n\r\n}\r\n",{"ruleId":"123","replacedBy":"124"},{"ruleId":"125","replacedBy":"126"},{"ruleId":"127","severity":1,"message":"128","line":27,"column":27,"nodeType":"129","endLine":27,"endColumn":36},{"ruleId":"127","severity":1,"message":"130","line":34,"column":6,"nodeType":"131","endLine":34,"endColumn":8,"suggestions":"132"},{"ruleId":"123","replacedBy":"133"},{"ruleId":"125","replacedBy":"134"},{"ruleId":"135","severity":1,"message":"136","line":20,"column":1,"nodeType":"137","endLine":31,"endColumn":2},{"ruleId":"135","severity":1,"message":"138","line":1,"column":1,"nodeType":"137","endLine":3,"endColumn":3},{"ruleId":"123","replacedBy":"139"},{"ruleId":"125","replacedBy":"140"},{"ruleId":"135","severity":1,"message":"136","line":21,"column":1,"nodeType":"137","endLine":33,"endColumn":2},{"ruleId":"135","severity":1,"message":"138","line":1,"column":1,"nodeType":"137","endLine":3,"endColumn":3},{"ruleId":"141","severity":1,"message":"142","line":54,"column":17,"nodeType":"129","messageId":"143","endLine":54,"endColumn":21},{"ruleId":"141","severity":1,"message":"142","line":54,"column":17,"nodeType":"129","messageId":"143","endLine":54,"endColumn":21},{"ruleId":"141","severity":1,"message":"144","line":19,"column":17,"nodeType":"129","messageId":"143","endLine":19,"endColumn":25},{"ruleId":